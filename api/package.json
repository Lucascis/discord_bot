{
  "name": "api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc --noEmit",
    "start": "node dist/index.js",
    "dev": "node --import 'data:text/javascript,import { register } from \"node:module\"; import { pathToFileURL } from \"node:url\"; register(\"ts-node/esm\", pathToFileURL(\"./\"));' src/index.ts"
  },
  "dependencies": {
    "@discord-bot/config": "workspace:*",
    "@discord-bot/database": "workspace:*",
    "@discord-bot/logger": "workspace:*",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.65.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.203.0",
    "@opentelemetry/sdk-node": "^0.203.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.7.0",
    "prom-client": "^15.1.3",
    "swagger-ui-express": "^5.0.1",
    "zod": "^3.25.76"
  },
  "optionalDependencies": {
    "@opentelemetry/resources": "^1.23.0",
    "@opentelemetry/semantic-conventions": "^1.23.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/ioredis": "^5.0.0",
    "@types/node": "^20.11.16",
    "@types/swagger-ui-express": "^4.1.8",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.8.4"
  }
}
