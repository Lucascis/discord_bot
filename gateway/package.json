{
  "name": "gateway",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "typecheck": "tsc --noEmit",
    "start": "node dist/main.js",
    "dev": "node --loader ts-node/esm src/index.ts",
    "prod": "NODE_ENV=production node dist/main.js"
  },
  "dependencies": {
    "@discord-bot/config": "workspace:*",
    "@discord-bot/database": "workspace:*",
    "@discord-bot/logger": "workspace:*",
    "@discord-bot/commands": "workspace:*",
    "@discord-bot/cqrs": "workspace:*",
    "@discord-bot/event-store": "workspace:*",
    "@discord-bot/cache": "workspace:*",
    "@discordjs/rest": "^1.7.1",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.62.1",
    "@opentelemetry/exporter-trace-otlp-http": "^0.203.0",
    "@opentelemetry/sdk-node": "^0.203.0",
    "discord.js": "^14.16.3",
    "express": "^4.19.2",
    "prom-client": "^15.1.3",
    "redis": "^5.8.2",
    "ioredis": "^5.4.1"
  },
  "optionalDependencies": {
    "@opentelemetry/resources": "^1.23.0",
    "@opentelemetry/semantic-conventions": "^1.23.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "ts-node": "^10.9.1",
    "typescript": "^5.8.4"
  }
}
