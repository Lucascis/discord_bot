{
  "name": "gateway",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "typecheck": "tsc --noEmit",
    "start": "node dist/index.js",
    "dev": "node --import 'data:text/javascript,import { register } from \"node:module\"; import { pathToFileURL } from \"node:url\"; register(\"ts-node/esm\", pathToFileURL(\"./\"));' src/index.ts"
  },
  "dependencies": {
    "@discord-bot/cache": "workspace:*",
    "@discord-bot/config": "workspace:*",
    "@discord-bot/cqrs": "workspace:*",
    "@discord-bot/database": "workspace:*",
    "@discord-bot/event-store": "workspace:*",
    "@discord-bot/logger": "workspace:*",
    "@discord-bot/subscription": "workspace:*",
    "@discordjs/rest": "^1.7.1",
    "@discordjs/voice": "^0.19.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.62.1",
    "@opentelemetry/exporter-trace-otlp-http": "^0.203.0",
    "@opentelemetry/sdk-node": "^0.208.0",
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "ioredis": "^5.4.1",
    "lavalink-client": "^2.5.9",
    "prom-client": "^15.1.3",
    "redis": "^5.8.2",
    "stripe": "^14.25.0"
  },
  "optionalDependencies": {
    "@opentelemetry/resources": "^1.23.0",
    "@opentelemetry/semantic-conventions": "^1.23.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "ts-node": "^10.9.1",
    "typescript": "^5.8.4"
  }
}
